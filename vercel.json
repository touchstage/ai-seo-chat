{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/apps/seo/chat-widget.js",
      "dest": "/public/chat-widget.js"
    },
    {
      "src": "/apps/seo/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/apps/seo/settings",
      "dest": "/api/settings.js"
    },
    {
      "src": "/app",
      "dest": "/api/admin.js"
    },
    {
      "src": "/app/(.*)",
      "dest": "/api/admin.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
